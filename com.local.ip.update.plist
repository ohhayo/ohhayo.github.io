<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.local.ip.update.plist</string>
	<key>ProgramArguments</key>
	<array>
		<string>/path/to/repo/update.sh</string>
	</array>
	<key>StartInterval</key>
    <integer>60</integer>
	<key>StandardOutPath</key>
	<string>/path/to/repo/log.txt</string>
	<key>StandardErrorPath</key>
	<string>/path/to/repo/err.txt</string>
</dict>
</plist>
